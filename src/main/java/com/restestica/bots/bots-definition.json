[
    {
        "botType": "TestCaseGenerator",
        "dockerImage": "bot-ica",
        "autonomy": {
            "type": "proactive",
            "initialDelay": 60,
            "period": 60
        },
        "keyToPublish": "testCasesGenerated",
        "orderToPublish": "executeTestCases",
        "rabbitOptions": {
            "mainQueue": "testCaseGenerator",
            "bindings": [
                "testOraclesGenerated"
            ],
            "queueByBot": true
        },
        "mount": [
            {
                "source": "./src/main/resources/Examples",
                "target": "/app/config/Examples"
            },
            {
                "source": "./src/main/resources/config.properties",
                "target": "/app/src/main/resources/config.properties"
            },
            {
                "source": "./src/main/resources/fuzzing-dictionary.json",
                "target": "/app/src/main/resources/fuzzing-dictionary.json"
            },
            {
                "source": "./output",
                "target": "/app/output"
            }
        ],
        "bots": [
            {
                "botId": "gen_4",
                "propertyFilePath": "/app/config/Examples/Ex4_CBTGeneration/user_config.properties",
                "isPersistent": true,
                "autonomy": {
                    "initialDelay": 120,
                    "period": 120
                }
            },
            {
                "botId": "gen_5",
                "propertyFilePath": "/app/config/Examples/Ex5_CBTGeneration/user_config.properties",
                "isPersistent": true
            }
        ]
    },
    {
        "botType": "TestCaseExecutor",
        "dockerImage": "bot-ica",
        "autonomy": {
            "type": "reactive",
            "order": "executeTestCases"
        },
        "keyToPublish": "testCasesExecuted",
        "orderToPublish": "generateTestReport",
        "rabbitOptions": {
            "mainQueue": "testCaseExecutor",
            "bindings": [
                "testCasesGenerated"
            ],
            "queueByBot": false
        },
        "mount": [
            {
                "source": "./output",
                "target": "/app/output"
            }
        ],
        "bots": [
            {
                "botId": "ex_1",
                "isPersistent": true
            },
            {
                "botId": "ex_2",
                "isPersistent": true
            }
        ]
    },
    {
        "botType": "TestReporter",
        "dockerImage": "bot-ica",
        "autonomy": {
            "type": "reactive",
            "order": "generateTestReport"
        },
        "keyToPublish": "testReportGenerated",
        "orderToPublish": "test",
        "rabbitOptions": {
            "mainQueue": "testReporter",
            "bindings": [
                "testCasesExecuted"
            ],
            "queueByBot": false
        },
        "mount": [
            {
                "source": "./allure",
                "target": "/app/allure"
            },
            {
                "source": "./src/main/resources/allure-categories.json",
                "target": "/app/config/allure-categories.json"
            },
            {
                "source": "./output",
                "target": "/app/output"
            }
        ],
        "bots": [
            {
                "botId": "re_1",
                "isPersistent": true
            },
            {
                "botId": "re_2",
                "isPersistent": true
            }
        ]
    }
]
